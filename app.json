    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/abclogo.png",
        "backgroundColor": "#080808"
      },
      "package": "com.hummasaeed.passengerapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e02c5af5-cf36-4486-8c84-64b5f8b23c87"
      }
    }
  }
}
