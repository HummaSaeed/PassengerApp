{
  "expo": {
    "name": "Passenger App",
    "slug": "passengerapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/abclogo.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#080808"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
